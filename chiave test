mettichiave.Name = "mettichiave"
mettichiave.Parent = Output
mettichiave.BackgroundTransparency = 0
mettichiave.BorderSizePixel = 1
mettichiave.BorderColor3 = Color3.fromRGB(255, 0, 0)
