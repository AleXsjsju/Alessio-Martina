mettichiave.MultiLine = false
local whatwhatwhatwhat = "Sp1510tR1513o"
local secretWord = whatwhatwhatwhat
local colorNormal = Color3.fromRGB(15, 15, 15)
local colorWrong = Color3.new(255, 0, 0)
local colorCorrect = Color3.new(0, 255, 0)
mettichiave.ClearTextOnFocus = true
mettichiave.Text = ""
mettichiave.Font = Enum.Font.Code
mettichiave.PlaceholderText = "Enter the Key"
mettichiave.BackgroundColor3 = colorNormal
local function onFocused()
	mettichiave.BackgroundColor3 = colorNormal
end

local function onFocusLost(enterPressed, _inputObject)
	if enterPressed then
		local guess = mettichiave.Text
		if guess == secretWord then
			mettichiave.Text = "ACCESS GRANTED"
			mettichiave.BackgroundColor3 = colorCorrect
            Options:Destroy()
            Background:Destroy()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/AleXsjsju/Project-Script-Roblox/main/code"))()
		else
			mettichiave.Text = "ACCESS DENIED"
			mettichiave.BackgroundColor3 = colorWrong
		end
	else
		mettichiave.Text = ""
		mettichiave.BackgroundColor3 = colorNormal
	end
end

mettichiave.FocusLost:Connect(onFocusLost)
mettichiave.Focused:Connect(onFocused)

